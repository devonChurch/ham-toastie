{
    "parser": "babel-eslint",
    "rules": {
        "linebreak-style": [2, "unix"],
        "no-console": 1,
        "no-unused-vars": 1,
        "quotes": [2, "single"],
        "semi": [2, "always"],
        "strict": 0,
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
